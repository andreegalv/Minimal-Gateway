<?xml version="1.0" encoding="utf-8"?>
<configuration>
	<packageSources>
		<clear />
		<add key="nuget.org" value="https://api.nuget.org/v3/index.json" />
		<add key="SigalAlmaPackages" value="https://nuget.pkg.github.com/SigalAlmaWeb/index.json" />
	</packageSources>
	<packageSourceMapping>
		<packageSource key="nuget.org">
			<package pattern="*" />
		</packageSource>
		<packageSource key="SigalAlmaPackages">
			<package pattern="SigalPackageNet.*" />
		</packageSource>
	</packageSourceMapping>
</configuration>
